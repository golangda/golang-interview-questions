{
  "id": null,
  "uid": "saga-lab",
  "title": "SAGA Choreography Lab",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "tags": [
    "saga",
    "kafka",
    "go"
  ],
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "DLQ messages / min",
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(dlq_messages_total[1m]))",
          "legendFormat": "DLQ rate"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto"
      }
    },
    {
      "type": "graph",
      "title": "Retries by reason (rate)",
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (step,reason) (rate(saga_retries_total[1m]))",
          "legendFormat": "step={{step}} reason={{reason}}"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 1,
      "points": false,
      "nullPointMode": "null as zero"
    },
    {
      "type": "graph",
      "title": "Step 5 latency p95",
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(saga_step_latency_seconds_bucket{{step=\"5\"}}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "nullPointMode": "null as zero"
    },
    {
      "type": "graph",
      "title": "Step throughput (approx)",
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum by (step) (rate(saga_step_latency_seconds_count[1m]))",
          "legendFormat": "step={{step}}"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 1,
      "nullPointMode": "null as zero"
    },
    {
      "type": "graph",
      "title": "DLQ rate",
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (topic) (rate(dlq_messages_total[1m]))",
          "legendFormat": "topic={{topic}}"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 1,
      "nullPointMode": "null as zero"
    },
    {
      "type": "graph",
      "title": "Retries (step 5 zoom)",
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (reason) (rate(saga_retries_total{step=\"5\"}[1m]))",
          "legendFormat": "reason={{reason}}"
        }
      ],
      "lines": true,
      "fill": 1,
      "linewidth": 1,
      "nullPointMode": "null as zero"
    }
  ]
}
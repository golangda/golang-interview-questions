{
  "id": null,
  "uid": "go-app-metrics",
  "title": "Go App Metrics (Prom Demo)",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Goroutines",
      "id": 1,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "go_goroutines",
          "legendFormat": "goroutines",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "GC Avg Duration (last 5m)",
      "id": 2,
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(go_gc_duration_seconds_sum[5m]) / rate(go_gc_duration_seconds_count[5m])",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Process RSS (bytes)",
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "CPU Seconds Rate",
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Scrape Requests by Code",
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (code) (rate(promhttp_metric_handler_requests_total[1m]))",
          "refId": "A"
        }
      ],
      "legend": {
        "show": true
      }
    },
    {
      "type": "timeseries",
      "title": "App Requests by Path",
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (path) (rate(app_requests_total[1m]))",
          "refId": "A"
        }
      ],
      "legend": {
        "show": true
      }
    },
    {
      "type": "timeseries",
      "title": "App Work Duration Histogram (rate)",
      "id": 7,
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "sum by (le) (rate(app_work_duration_seconds_bucket[5m]))",
          "refId": "A"
        }
      ],
      "legend": {
        "show": true
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  }
}